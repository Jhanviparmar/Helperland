@model Helperland.Models.ServiceRequest;

@Html.Raw(ViewBag.Message)
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="~/css/book-a-serviceStyle.css">
    <link rel="stylesheet" type="text/css" href="~/css/styles.css">
    <title>Book Service | Helperland</title>
    <!--------------Bootstrap------------------>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>
    <!------------Navbar-------------->
    @Html.Partial("_header")
    <div class="alert">@ViewBag.msg</div>
    <!------------------------HEADING------------------>
    <div class="d-flex flex-column text-center align-items-center justify-content-center main-content">
        <h2>Set up your cleaning service</h2>
        <div class="d-flex text-center align-items-center justify-content-center pb-3">
            <div class="separator-border me-2"></div>
            <img src="~/images/separator.png">
            <div class="separator-border ms-2"></div>
        </div>
    </div>
    <!-----------------------MAIN-CONTENT--------------->
    <section class="container">
        <div class="row">
            <div class="col-md-8 col-12 service">
                <ul class="nav nav-pills service-tabs mb-3" id="pills-tab" role="tablist">
                    <li class="nav-item" role="presentation">
                        <button class="nav-link active d-xl-flex d-block pt-3" id="tab1" data-bs-toggle="pill" data-bs-target="#setup-service" type="button" role="tab" aria-controls="pills-home" aria-selected="true">
                            <img src="~/images/setup-service.png">
                            <p class="d-md-block d-none pt-xl-2">Setup Service</p>
                        </button>
                    </li>
                    <li class="nav-item" role="presentation">
                        <button class="nav-link d-xl-flex d-block pt-3 disabled" id="tab2" data-bs-toggle="pill" data-bs-target="#schedule-plan" type="button" role="tab" aria-controls="pills-profile" aria-selected="false">
                            <img src="~/images/schedule-plan.png" class="me-1">
                            <p class="d-lg-block d-none pt-xl-2">Schedule & Plan</p>
                            <p class="d-lg-none d-md-block d-none pt-xl-2">Schedule</p>
                        </button>
                    </li>
                    <li class="nav-item" role="presentation">
                        <button class="nav-link d-xl-flex d-block pt-3 disabled" id="tab3" data-bs-toggle="pill" data-bs-target="#your-details" type="button" role="tab" aria-controls="pills-contact" aria-selected="false">
                            <img src="~/images/user-details.png">
                            <p class="d-lg-block d-none pt-xl-2">Your Details</p>
                            <p class="d-lg-none d-md-block d-none pt-xl-2"> Details </p>
                        </button>
                    </li>
                    <li class="nav-item" role="presentation">
                        <button class="nav-link d-xl-flex d-block pt-3 disabled" id="tab4" data-bs-toggle="pill" data-bs-target="#make-payment" type="button" role="tab" aria-controls="pills-contact" aria-selected="false">
                            <img src="~/images/payment.png" class="me-2">
                            <p class="d-lg-block d-none pt-xl-2">Make Payment</p>
                            <p class="d-lg-none d-md-block d-none pt-xl-2"> Payment </p>
                        </button>
                    </li>
                </ul>
                <div class="tab-content" id="pills-tabContent">
                    <div class="tab-pane active flex-column fade show" id="setup-service">
                        <div class="col p-3">
                            <div class="fw-bold mb-2" style="font-size:18px; color:#646464">Enter your Postal Code</div>
                            <div class="flex-row">
                                <input type="text" id="postalcode" name="Postalcode" placeholder="Postal Code" class="p-2 me-3 postal mb-3" @*asp-for="ZipcodeValue*@>
                                <button type="button" class="button px-4 py-2" id="button1">
                                    Check Availability
                                </button>
                                <div class="text-danger" id="errormessage"></div>
                            </div>
                        </div>
                    </div>

                    <div class="tab-pane flex-column" id="schedule-plan">
                        @using (Html.BeginForm("SchedulePlan", "Home", FormMethod.Post))
                        {
                            <div class="col p-4">
                                <div class="row">
                                    <div class="col">
                                        <div class="flex-column">
                                            <div class="fw-bold mb-3">When do you want your helper to be with you?</div>
                                            <div>
                                                <input type="date" class="p-2 me-2 mb-2" id="ServiceStartDate">
                                                @*<span><input type="time" name="appt" id="servicestarttime"></span>*@
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <div class="flex-column">
                                            <div class="fw-bold mb-3">How long do you need your helper?</div>
                                            <div>
                                                <select name="time" class="p-2" id="ServiceHours">
                                                    <option value="3.0 Std">3.0 Std</option>
                                                    <option value="3.5 Std">3.5 Std</option>
                                                    <option value="4.0 Std">4.0 Std</option>
                                                    <option value="4.5 Std">4.5 Std</option>
                                                    <option value="5.0 Std">5.0 Std</option>
                                                    <option value="5.5 Std">5.5 Std</option>
                                                    <option value="6.0 Std">6.0 Std</option>
                                                    <option value="6.5 Std">6.5 Std</option>
                                                    <option value="7.0 Std">7.0 Std</option>
                                                    <option value="7.5 Std">7.5 Std</option>
                                                    <option value="8.0 Std">8.0 Std</option>
                                                    <option value="8.5 Std">8.5 Std</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <hr style="height:2px">
                                <div class="container extra-service">
                                    <div class="fw-bold mb-4" style="color: #646464; font-size:18px">Extra Services</div>
                                    <div class="row">
                                        <div class="col ">
                                            <div class="ischecked pt-4 border1" value="0" target="false" target2="InsideCabinet" id="s1">
                                                <img class="pe-5" src="~/images/p1.png">
                                            </div>
                                            <p class="inside">Inside cabinets</p>
                                        </div>
                                        <div class="col">
                                            <div class="ischecked pt-4 border1" value="0" target="false" target2="InsideFridge" id="s2">
                                            <img src="~/images/p2.png"></div>
                                            <p class="inside">Inside fridge</p>
                                        </div>
                                        <div class="col">
                                            <div class="ischecked p3 ps-4 border1" value="0" target="false" target2="InsideOven" id="s3">
                                            <img src="~/images/p3.png"></div>
                                            <p class="inside">Inside oven</p>
                                        </div>
                                        <div class="col">
                                            <div class="ischecked pt-4 border1" value="0" target="false" target2="InsideLaundry" id="s4">
                                            <img src="~/images/p4.png"></div>
                                            <p class="inside">Laundry wash & dry</p>
                                        </div>
                                        <div class="col">
                                            <div class="ischecked p5 pt-4 border1" value="0" target="false" target2="InsideWindow" id="s5">
                                            <img src="~/images/p5.png"></div>
                                            <p class="inside">Interior windows</p>
                                        </div>
                                    </div>
                                </div>
                                <hr style="height:2px">
                                <div class="d-flex flex-column fw-bold" style="color: #646464; font-size:18px">Comments</div>
                                <div class="form-outline w-90">
                                    <textarea class="form-control" rows="3" id="Comments"></textarea>
                                    <label class="form-label comments" for="textAreaExample6"></label>
                                </div>
                                <div class="mt-3">
                                    <input type="checkbox" id="pets">
                                    <label for="pets" style="color: #646464; font-size:18px"> I have pets at home</label><br>
                                </div>
                                <hr style="height:2px">
                                <div>
                                    <button type="button"
                                            class="d-inline-flex continue button px-4 py-2" id="button2">
                                        Continue
                                    </button>
                                </div>
                            </div>
                        }

                    </div>

                    <div class="tab-pane flex-column" id="your-details">

                    </div>

                    <div class="tab-pane" id="make-payment">

                        <div class="col flex-column p-3">
                            <div class="flex-column">
                                <div class="fw-bold mb-2" style="font-size:18px">Choose one of the following payment methods.</div>
                                <div class="mb-2">
                                    <label for="dcode" class="mb-2">Discount code (optional)</label><br>
                                    <input type="text" id="fname" name="fname" placeholder="Discount code (optional)" class="p-2 me-3 mb-2">
                                    <span><button class="use py-2 px-4">Use</button></span>
                                </div>
                            </div>
                            <hr style="height:2px">
                            <div class="card px-2 py-1 my-3">
                                <div class="d-flex navbar flex-row">
                                    <div>
                                        <input name="credit-number" class="cc-number" type="tel" pattern="\d*" maxlength="19" placeholder="Card Number" id="card_number">
                                    </div>
                                    <div>
                                        <input type="text" name="month" placeholder="MM" maxlength="2" size="2" />
                                        <span>/</span>
                                        <input type="text" name="year" placeholder="YY" maxlength="2" size="2" />
                                        <input type="text" name="cvv" placeholder="CVV" maxlength="4">
                                    </div>
                                </div>
                            </div>
                            <div class="cards flex-column">
                                <div>Accepted cards:</div>
                                <div><img src="~/images/credit-cards.png"></div>
                            </div>
                            <hr style="height:2px">
                            <div>
                                <label for="pets" style="color: #646464; font-size:14px">
                                    <input type="checkbox" required>
                                    I accept the terms and conditions , the cancellation policy and the data protection regulations .
                                    I confirm that Helperland will start executing the contract before the end of the cancellation
                                    period and that I will lose my right of cancellation as a consumer once the contract has been fully performed.
                                </label>
                            </div>
                            <hr style="height:2px">
                            <button type="button" class="d-inline-flex continue button px-4 py-2" id="booking-btn" data-toggle="modal" data-target="#exampleModal">
                                Book with costs
                            </button>
                            <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5></h5>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="border:none; font-size: 36px; background-color:#fff; color: #4f4f4f;">
                                                <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                        <div class="modal-body text-center align-items-center justify-content-center">
                                            <img src="~/images/green-tick.png" style="height:50px; width:50px;">
                                            <div style="font-size: 20px">The booking was successfully sent</div>
                                            <div>Service ID : <span id="sidd"></span></div>
                                            <a type="button" style="text-decoration : none; color:#fff;" class="button px-4 py-2 my-2" asp-action="sdashboard" asp-controller="Home">OK</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

            <!--------------------------Payment---------------------------->
            <div class="col-md-4 col-12 text-left d-md-block d-none">
                <div class="payment flex-column mb-3">
                    <p class="p-2 ps-3">Payment Summary</p>
                    <div class="d-flex navbar flex-row px-3">
                        <div>Duration <br>Basic</div>
                        <div>3 Hrs</div>
                    </div>
                    <div class="d-flex navbar flex-row px-3 d-none" id="InsideCabinet">
                        <div>Clean cabinet interiors</div>
                        <div>30 Min.</div>
                    </div>
                    <div class="d-flex navbar flex-row px-3 d-none" id="InsideFridge">
                        <div>Cleaning the fridge</div>
                        <div>30 Min.</div>
                    </div>
                    <div class="d-flex navbar flex-row px-3 d-none" id="InsideOven">
                        <div>Cleaning the oven</div>
                        <div>30 Min.</div>
                    </div>
                    <div class="d-flex navbar flex-row px-3 d-none" id="InsideLaundry">
                        <div>Washing and drying laundey</div>
                        <div>30 Min.</div>
                    </div>
                    <div class="d-flex navbar flex-row px-3 d-none" id="InsideWindow">
                        <div>Clean windows</div>
                        <div>30 Min.</div>
                    </div>
                    <hr style="width:90%" class="ms-2">
                    <div class="d-flex navbar flex-row px-3">
                        <div>Total Service Time</div>
                        <div><span id="TotalHours">3</span> Hrs</div>
                    </div>
                    <hr style="height:2px">
                    <div class="d-flex navbar flex-row px-3">
                        <div>Per cleaning</div>
                        <div><span id="SubTotal">0.00</span>€</div>
                    </div>
                    <hr>
                    <div class="d-flex flex-row total-payment navbar px-3">
                        <div>Total Payment</div>
                        <div class="price fw-bold"><span id="TotalPayment">0.00</span> €</div>
                    </div>
                    <div class="smily py-2 ps-2"><img class="me-2" src="~/images/smiley.png">Take a look at our included basic services</div>
                </div>
                <div class="questions" style=" color:#646464">
                    <div class="text-center pb-3" style="font-size:18px; color:#4f4f4f">Questions</div>
                    <div>
                        <a class="fc fw-bold " data-bs-toggle="collapse" href="#fc1" role="button" aria-expanded="false" aria-controls="collapseExample">
                            <img class="me-2" src="~/images/right-arrow.png">
                        </a>
                        What is included in basic cleaning?
                        <hr style="width:90%" class="ms-2">
                        <div class="collapse mb-3 ms-3" id="fc1">
                            Living room, bedroom and bathroom, kitchen and common rooms
                        </div>
                    </div>
                    <div>
                        <a class="fc fw-bold " data-bs-toggle="collapse" href="#fc2" role="button" aria-expanded="false" aria-controls="collapseExample">
                            <img class="me-2" src="~/images/right-arrow.png">
                        </a>
                        Can I skip or more bookings?
                        <hr style="width:90%" class="ms-2">
                        <div class="collapse mb-3 ms-3" id="fc2">
                            You can change a booking free of charge 48 hours before an assignment. If you want to skip an already booked assignment, we will credit the value of the booking to your account. You can use the credit for a future booking.
                        </div>
                    </div>
                    <div class="thirty"><a href="faqs.html">More questions and answers</a></div>
                </div>
            </div>
            <div class="d-md-none d-block text-center align-items-center justify-content-center">
                <button class="payment-btn px-3 py-1" data-toggle="modal" data-target="#payment">Payment summary</button>
                <div class="modal fade payment flex-column" id="payment" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title" id="exampleModalLabel">Payment Summary</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="border : none; background-color: #1d7c8a; font-size: 36px; color: #fff;">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <div class="d-flex navbar flex-row px-3">
                                    <div>Duration <br>Basic</div>
                                    <div>3 Hrs</div>
                                </div>
                                <div class="d-flex navbar flex-row px-3 d-none" id="InsideCabinet1">
                                    <div>Clean cabinet interiors</div>
                                    <div>30 Min.</div>
                                </div>
                                <div class="d-flex navbar flex-row px-3 d-none" id="InsideFridge1">
                                    <div>Cleaning the fridge</div>
                                    <div>30 Min.</div>
                                </div>
                                <div class="d-flex navbar flex-row px-3 d-none" id="InsideOven1">
                                    <div>Cleaning the oven</div>
                                    <div>30 Min.</div>
                                </div>
                                <div class="d-flex navbar flex-row px-3 d-none" id="InsideLaundry1">
                                    <div>Washing and drying laundey</div>
                                    <div>30 Min.</div>
                                </div>
                                <div class="d-flex navbar flex-row px-3 d-none" id="InsideWindow1">
                                    <div>Clean windows</div>
                                    <div>30 Min.</div>
                                </div>
                                <hr style="width:90%" class="ms-2">
                                <div class="d-flex navbar flex-row px-3">
                                    <div>Total Service Time</div>
                                    <div><span id="TotalHours1">3</span>  Hrs</div>
                                </div>
                                <hr style="height:2px">
                                <div class="d-flex navbar flex-row px-3">
                                    <div>Per cleaning</div>
                                    <div><span id="SubTotal1">0.00</span> €</div>
                                </div>
                                <hr>
                                <div class="d-flex flex-row total-payment navbar px-3">
                                    <div>Total Payment</div>
                                    <div class="price fw-bold"><span id="TotalPayment1">0.00</span> €</div>
                                </div>
                                <div class="smily py-2 ps-2"><img class="me-2" src="~/images/smiley.png">Take a look at our included basic services</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>

    <div onclick="topFunction()" id="myBtn" title="Go to top"><img class="scroll ms-5" src="~/images/up.png"></div>


    <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="https://code.jquery.com/jquery-1.7.2.min.js"></script>


    <script src="~/js/mobilemenu.js"></script>
    <script src="~/js/script.js"></script>
    <script type="text/javascript">
        window.addEventListener("scroll", function () {
            var header = document.querySelector("header");
            header.classList.toggle("sticky", window.scrollY > 0);
        });
    </script>
    <script type="text/javascript">
        card_number.addEventListener('keyup', function (e) {
            // console.log(e.keyCode);
            if (e.keyCode !== 8) {
                if (this.value.length === 4 || this.value.length === 9 || this.value.length === 14) {
                    this.value = this.value += ' ';
                }
            }
        });
    </script>
    <script language="javascript">
        
        var today = new Date();
        var dd = String(today.getDate()).padStart(2, '0');
        var mm = String(today.getMonth() + 1).padStart(2, '0');
        var yyyy = today.getFullYear();

        today = yyyy + '-' + mm + '-' + dd;
        $('#ServiceStartDate').attr('min', today);
    </script>
    <script>
            
            $('#button1').on('click', function () {
                var postalcode = $('#postalcode').val();
                $.ajax({
                    url: '@Url.Action("PostalCode","Home")',
                    type: 'post',
                    data: {
                        postalcode: $('#postalcode').val()
                    },
                    success: function (res) {
                        if (res == 'true') {
                            document.getElementById("tab2").classList.remove("disabled");
                            $('#button1').click(function () {
                                $('#tab2').click();
                            });
                        }
                        else {
                            $('#errormessage').html('We are not providing service in this area. We’ll notify you if any helper would start working near your area.').fadeOut(10000);
                        }
                    },
                    error: function (err) {
                        console.log(err);
                    }
                });
            });

        $(document).on('click', '#button2', function () {
            document.getElementById("tab3").classList.remove("disabled");
                $("#your-details").html("Loading Addresses...")
            .load('@Url.Action("Details", "Home")');
            });

        $('#button2').click(function () {
            document.getElementById("tab3").classList.remove("disabled");
                $('#tab3').click();
            });

        $('#button3').click(function () {
            document.getElementById("tab4").classList.remove("disabled");
                $('#tab4').click();
        });

        //$('#ServiceStartDate').datetimepicker();
        //$("#ServiceStartDate").on("click", function (e) {
        //    $('#ServiceStartDate').data("DateTimePicker").minDate(new Date());
        //});

    </script>
    <script>
            function hidefunction() {
                document.getElementById("address").classList.add("d-none");
                document.getElementById("address-box").classList.remove("d-none");
            }

            function discard() {
                document.getElementById("address").classList.remove("d-none");
                document.getElementById("address-box").classList.add("d-none");
        }



            $(document).on('click', '#save-address', function () {
                var address = {};
                address.AddressLine1 = $('#AddressLine1').val();
                address.AddressLine2 = $('#AddressLine2').val();
                address.PostalCode = $('#PostalCode').val();
                address.city = $('#City').val();
                address.mobile = $('#Mobile').val();

                $.ajax({
                    url: '@Url.Action("SaveAddress","Home")',
                    type: 'post',
                    dataType: 'json',
                    contentType: 'application/json',
                    data: JSON.stringify(address),
                    success: function (res) {
                        if (res == true) {
                            $("#your-details").html("Updating Address...").load('@Url.Action("Details", "Home")');
                        }
                        else if (res == false) {
                            alert("Something went wrong while adding address..");
                        }
                    },
                    error: function(err) {
                        console.log('error in ajax');
                    }
                });
            });


            var price = 54;
            var totalTime = 3;
        $('#TotalPayment').html(price);
        $('#TotalPayment1').html(price);
        $('#SubTotal').html(price);
        $('#SubTotal1').html(price);
            $('.ischecked').click(function () {
                var extraservice = $(this).attr('target');
                var selectedservice = $(this).attr('target2');
                if (extraservice == "false") {
                    $(this).attr('target', 'true');
                    $(this).css({
                        'border': 'solid 2px #1d7a8c',
                    });
                    $(this).attr('value', '1');
                    $('#' + selectedservice).removeClass('d-none');
                    $('#' + selectedservice + "1").removeClass('d-none');

                    price = price + 9;
                    totalTime = parseFloat(totalTime) + 0.5;
                    console.log(extraservice);
                    console.log(selectedservice);
                }
                else {
                    $(this).attr('target', 'false');
                    $(this).css({
                        'border': 'solid 2px #b0c9ce',
                    });
                    $(this).attr('value', '0');
                    $('#' + selectedservice).addClass('d-none');
                    $('#' + selectedservice + "1").addClass('d-none');
                    price = price - 9;
                    totalTime = parseFloat(totalTime) - 0.5;
                    console.log(extraservice);
                }
                $('#SubTotal').html(price);
                $('#TotalPayment').html(price);
                $('#TotalHours').html(totalTime);
                $('#SubTotal1').html(price);
                $('#TotalPayment1').html(price);
                $('#TotalHours1').html(totalTime);
            });
